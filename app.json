{
  "id": "nl.deskweb.netcheck",
  "version": "1.0.2",
  "compatibility": ">=1.5.0",
  "sdk": 2,
  "name": {
    "en": "Network Check",
    "nl": "Netwerk Controle"
  },
  "description": {
    "en": "Keeps an eye on your network",
    "nl": "Houdt je netwerk in de gaten"
  },
  "category": [
    "tools"
  ],
  "permissions": [
    "homey:manager:ledring"
  ],
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "author": {
    "name": "Gabri van Lee",
    "email": "gvanlee@gmail.com"
  },
  "flow": {
    "triggers": [
      {
        "id": "connection_lost",
        "title": {
          "en": "Internet connection lost",
          "nl": "Internetverbinding verbroken"
        }
      },
      {
        "id": "connection_restored",
        "title": {
          "en": "Internet connection restored",
          "nl": "Internetverbinding hersteld"
        }
      }
    ]
  }
}